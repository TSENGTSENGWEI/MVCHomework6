@model IEnumerable<MVCHomework6.Data.Database.TagCloud>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    foreach (var item in Model)
    {
        <a asp-controller="Home" asp-action="FindTag" asp-route-tag="@item.Name">@item.Name (@item.Amount)</a>
    }

}


