@{
}
<ul>
    @{
        var split = Model.Tags.Split(',');
        foreach(var item in split)
        {
            <li class="d-inline p-2">
                @*<a href="@Model.OnGetTag(@item)">@item</a>*@
                <tags>@item</tags>
            </li>
        }
    }    
</ul>

